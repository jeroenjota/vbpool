
h2.header Achtste finales &nbsp;
  p.small (#{weds.punten.achtsteNaam} pnt voor elk genoemd team of #{weds.punten.achtstePlaats} pnt als ie ook op de goeie plaats staat)
table.finals(width="100%")
  tr.finals
  each wed in weds.wedstrijden
    if wed.soort === "Achtste finales"
      td.nr #{wed.nr}:
      td
        label #{wed.naam1}:
          select(width="25%")
            each team in teams
              option= team
      td.nr vs
      td 
        label #{wed.naam2}:
          select(width="60px")
            each team in teams
              option= team
      if wed.nr%2 == 0
        tr.finals
